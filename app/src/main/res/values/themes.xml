<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.GithubUser" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#3b6939</item>
        <item name="colorPrimaryVariant">#52634f</item>
        <item name="colorOnPrimary">#ffffff</item>
        <item name="colorPrimaryContainer">#bcf0b4</item>
        <item name="colorOnPrimaryContainer">#002204</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#52634f</item>
        <item name="colorSecondaryVariant">#52634f</item>
        <item name="colorOnSecondary">#ffffff</item>
        <item name="colorSecondaryContainer">#d5e8cf</item>
        <item name="colorOnSecondaryContainer">#111f0f</item>

        <!-- tertiary brand color. -->
        <item name="colorTertiary">#38656a</item>
        <item name="colorOnTertiary">#ffffff</item>
        <item name="colorTertiaryContainer">#bcebf0</item>
        <item name="colorOnTertiaryContainer">#002023</item>


        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="colorSurface">#f7fbf1</item>
        <item name="colorOnSurface">#191d17</item>

    </style>
    <style name="Theme.GithubUser.NoActionBar" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="windowActionBar">false</item>
    </style>

</resources>